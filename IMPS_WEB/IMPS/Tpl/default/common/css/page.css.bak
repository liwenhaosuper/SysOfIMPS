@charset "utf-8";

ul.thumb li {
margin: 5px 8px;
margin: 5px 7px	9;
padding: 5px;
float: left;
width: 200px;
height: 350px;
position: relative;
overflow: hidden;
}
ul.thumb li img {
width: 190px;
border: 1px solid #DDD;
padding: 5px;
background: #F0F0F0;
position: absolute;
left: 0;
top: 0;
}
ul.thumb li p {
margin-top: 3px;
padding: 5px;
width: 190px;
height: 100px;
background: white;
border: 1px solid #DDD;
overflow: hidden;
position: absolute;
left: 0;
top: 210px;
}

.clr{clear:both;}
/*CSS green-black style pagination*/
DIV#green-black {
	PADDING-RIGHT: 3px; PADDING-BOTTOM: 3px; PADDING-TOP: 3px; TEXT-ALIGN: left; margin-top:10px;
}
DIV#green-black A {
	BORDER-RIGHT: #2c2c2c 1px solid; PADDING-RIGHT: 5px; BORDER-TOP: #2c2c2c 1px solid; PADDING-LEFT: 5px; BACKGROUND: url(../Images/image1.gif) #2c2c2c; PADDING-BOTTOM: 2px; BORDER-LEFT: #2c2c2c 1px solid; COLOR: #fff; MARGIN-RIGHT: 2px; PADDING-TOP: 2px; BORDER-BOTTOM: #2c2c2c 1px solid; TEXT-DECORATION: none
}
DIV#green-black A:hover {
	BORDER: #f3f3f3 1px solid; BACKGROUND: #aad83e; COLOR: #fff;
}
DIV#green-black A:active {
	BORDER: #f3f3f3 1px solid; BACKGROUND: #aad83e; COLOR: #fff;
}
DIV#green-black span.current {
	BORDER: #f3f3f3 1px solid; PADDING-RIGHT: 5px;  PADDING-LEFT: 5px; FONT-WEIGHT: bold; BACKGROUND: #aad83e; PADDING-BOTTOM: 2px; COLOR: #fff; MARGIN-RIGHT: 2px; PADDING-TOP: 2px;
}
DIV#green-black span.disabled {
	BORDER-RIGHT: #f3f3f3 1px solid; PADDING-RIGHT: 5px; BORDER-TOP: #f3f3f3 1px solid; PADDING-LEFT: 5px; PADDING-BOTTOM: 2px; BORDER-LEFT: #f3f3f3 1px solid; COLOR: #ccc; MARGIN-RIGHT: 2px; PADDING-TOP: 2px; BORDER-BOTTOM: #f3f3f3 1px solid
}
DIV#green-black input{
    background: #FFFFFF;
    border: 1px solid #CCCCCC;
    color: #000000;
    font-family: tahoma, sans-serif;
    font-size: 12px;
	vertical-align: middle;
	height:16px;
	padding-bottom:2px;
	padding-top:2px;
	width:25px;
}
DIV#green-black select{
    width: 50px; 
 	height:22px;
    color: #000000;
	vertical-align: middle;
    border:1px solid #CCCCCC;
}